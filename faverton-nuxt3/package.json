{
  "name": "realestate-public-site",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "vitest",
    "test:unit": "vitest run app/",
    "test:integration": "vitest run app/",
    "test:e2e": "playwright test",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "exec playwright show-report",
    "test:e2e:clean": "rm -rf test-results playwright-report blob-report",
    "test:e2e:clean-git": "git rm --cached -r playwright-report/ test-results/ 2>/dev/null || true",
    "test:all": "pnpm test:unit && pnpm test:integration && pnpm test:e2e"
  },
  "dependencies": {
    "@nuxt/ui": "^2.21.1",
    "@nuxtjs/cloudinary": "^4.0.0",
    "@nuxtjs/leaflet": "^1.2.6",
