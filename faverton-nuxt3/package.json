    "@pinia/nuxt": "^0.10.1",
    "@supabase/supabase-js": "^2.49.4",
    "@tresjs/cientos": "^3.9.0",
    "@tresjs/nuxt": "^3.0.8",
    "@tresjs/post-processing": "^0.7.1",
    "chart.js": "^4.4.8",
    "gsap": "^3.12.7",
    "leaflet": "^1.9.4",
    "leaflet-draw": "^1.0.4",
    "nuxt": "^3.16.1",
    "pinia": "^3.0.1",
    "vue": "latest",
    "vue-chartjs": "^5.3.2"
  },
  "packageManager": "pnpm@9.5.0+sha1.8c155dc114e1689d18937974f6571e0ceee66f1d",
